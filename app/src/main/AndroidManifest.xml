        <activity android:name="dk.events.a6.profileView.EditProfileActivity" />
        <activity android:name="dk.events.a6.profileView.MyAccount" />

        <activity android:name=".signInView.Registeration">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>

</manifest>
