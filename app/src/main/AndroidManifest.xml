        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:screenOrientation="portrait">
 
        <activity android:name="dk.events.a6.android.usecases.createevent.CreateEventActivityView"></activity>
        <activity android:name="dk.events.a6.activities.Event_Content"></activity>
